{
  "vertical sine wave test":[
    {
      "spawner":{
        "vWave":{
          "delay":40,
          "size":"((c + 8)**(1/3))/500",
          "lifeTime":13000,
          "x":"0.5*(1+sin(t*pi))",
          "y":"0.1*t - 0.1*(1+cos((t+0.04*c)*pi*0.25))"
        }
      }
    },
    {
      "wait":33334
    },
    {
      "del":["vWave"]
    }
  ],
  "8-leaved spiral":[
    {
      "spawner":{
        "revolvingCircle":{
          "delay":12,
          "size":"0.01",
          "x":"0.5+0.32*cos(0.248*pi*c)+0.32*cos(t*pi+c*0.248*pi+pi)",
          "y":"0.5+0.32*sin(0.248*pi*c)+0.32*sin(t*pi+c*0.248*pi+pi)",
          "lifeTime":2000
        }
      }
    },
    {
      "wait":5000
    },
    {
      "del":["revolvingCircle"]
    }
  ],
  "pulse!":[
    {
      "pattern":{
        "pulse":{
          "count":50,
          "size":"0.01",
          "fadein":500,
          "x":"0.5 + 0.5*cos(0.04*c*pi)*t*cos((.1*(c%2)+.45)*t*pi)",
          "y":"0.2 + 0.5*sin(0.04*c*pi)*t*cos((.1*(c%2)+.45)*t*pi)",
          "lifeTime":2000
        },
        "wave":{
          "count":80,
          "size":"0.01",
          "fadein":0,
          "x":"(c+0.5)/80",
          "y":"1 - 0.2 * (2-sin(pi*(c+0.5)/80)) * sin(t*pi)",
          "lifeTime":2000
        },
        "follower":{
          "count":5,
          "size":".01",
          "x":"px + 0.1 * (1.5+sin(t*pi)) * cos((t+c*.2)*pi*2)",
          "y":"py + 0.1 * (1.5+sin(t*pi)) * sin((t+c*.2)*pi*2)",
          "lifeTime":10000
        }
      }
    },
    {
      "trigger":["pulse", "wave","follower"]
    },
    {
      "wait":500
    },
    {
      "trigger":["pulse"]
    },
    {
      "wait":500
    },
    {
      "del":["pulse", "wave"]
    }
  ],
  "predictable bullet hell":[
    {
      "spawner":{
        "microspiral":{
          "delay":50,
          "size":"0.012",
          "x":"0.5+0.5*sin(t*pi)",
          "y":"0.1*t+0.2*sin(t*2*pi)",
          "lifeTime":20000
        },
        "Intervention":{
          "delay":400,
          "size":"0.06",
          "x":"0.5 + 0.5 * cos(c*1.19*pi)",
          "y":"t/4",
          "lifeTime":4000
        }
      }
    },
    {
      "wait":10000
    },
    {
      "del":["microspiral", "Intervention"]
    }
  ],
  "6-pointed sharp star":[
    {
      "spawner":{
        "microspiral":{
          "delay":40,
          "size":"0.022",
          "x":"0.5 + 0.2*sqrt(2*t+0.03)*cos(c*0.246*pi)",
          "y":"0.5 + 0.2*sqrt(2*t+0.03)*sin(c*0.246*pi)",
          "lifeTime":4000
        }
      }
    },
    {
      "wait":10000
    },
    {
      "del":["microspiral"]
    }
  ],
  "boxTest":[
    {
      "spawner":{
        "testspawner":{
          "delay":40,
          "x":"(c/20)%1",
          "y":"floor(c/20)/5",
          "dx":".05 * (2+sin(t*2*pi))",
          "dy":".05",
          "lifeTime":2000
        }
      }
    },
    {
      "wait":5000
    },
    {
      "del":["testspawner"]
    }
  ]
}
